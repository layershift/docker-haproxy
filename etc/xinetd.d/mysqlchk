# default: on
# description: mysqlchk
service mysqlchk
{
        disable = no
        flags = REUSE
        socket_type = stream
        port = 9200
        wait = no
        user = nobody
        server = /usr/bin/clustercheck
        log_type = FILE /dev/null
        log_on_failure += USERID
        only_from = 0.0.0.0/0
        bind = 0.0.0.0
        per_source = UNLIMITED
}
